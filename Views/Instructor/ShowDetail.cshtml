
@model Instructor
<h1>Instructor Details</h1>
<table class="table table-dark">
    <tr>
        <th>ID</th>
        <td>@Model.ID</td>
    </tr>
    <tr>
        <th>
            Last Name
        </th>
        <td>@Model.LastName</td>
    </tr>
    <tr>
        <th>
            Firt Name
        </th>
        <td>@Model.FirstName</td>
    </tr>
    <tr>
        <th>
            Rank
        </th>
        <td>@Model.Rank</td>
    </tr>

</table>

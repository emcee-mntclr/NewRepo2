@model Student
<h1>Student Details</h1>
<table class="table table-dark">
    <tr>
        <th>ID</th>
        <td>@Model.id</td>
    </tr>
    <tr>
        <th>
            First Name
        </th>
        <td>@Model.Fname</td>
    </tr>
    <tr>
        <th>
            Last Name
        </th>
        <td>@Model.Lname</td>
    </tr>
    <tr>
        <th>
            Date Enrolled
        </th>
        <td>@Model.dateenrolled</td>
    </tr>
    <tr>
        <th>
            Course
        </th>
        <td>@Model.course</td>
    </tr>
    <tr>
        <th>
            email
        </th>
        <td>@Model.email</td>
    </tr>

</table>
